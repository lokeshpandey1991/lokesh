# Configuration created by Apache Sling JCR Installer
handler.schemes=["jcrinstall","launchpad"]
sling.jcrinstall.enable.writeback=B"true"
sling.jcrinstall.search.path=["/libs:100","/apps:200"]
sling.jcrinstall.folder.name.regexp=".*/(install|config)$"
sling.jcrinstall.signal.path="/system/sling/installer/jcr/pauseInstallation"
sling.jcrinstall.new.config.path="system/config"
sling.jcrinstall.folder.max.depth=I"6"