<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Assay Menu"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <section
                jcr:primaryType="nt:unstructured"
                jcr:title="Assay Menu component"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <headline
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Assay Menu Title"
                                        fieldLabel="Enter Title"
                                        name="./title"/>
									<compareHeadline
									jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/foundation/form/textfield"
									fieldDescription="Assay Menu Compare Title"
									fieldLabel="Enter Compare Title"
									name="./compareTitle"/>
                                    <multi
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/multifield"
                                        class="full-width"
                                        fieldDescription="Click 'Add fiels to add Question answer' for FAQ"
                                        fieldLabel="Add Question answer">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                            acs-commons-nested="JSON_STORE"
                                            name="./products">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                method="absolute"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <productid
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldDescription="Instument ID"
                                                            fieldLabel="Instument Id"
                                                            name="./productId"/>
                                                        <productname
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldDescription="Instument Name"
                                                            fieldLabel="Instument Name"
                                                            name="./productName"/>
                                                        <productimage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            fieldDescription="Enter Instument image path"
                                                            fieldLabel="Instument Image"
                                                            name="./productImage"
                                                            required="{Boolean}true"
                                                            rootPath="/content/dam/customerportal"/>
                                                        <alttext
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldDescription="Instument Image Alt Text"
                                                            fieldLabel="Instument Image Alt Text"
                                                            name="./altText"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </multi>
                                </items>
                            </column>
                        </items>
                    </column>
                </items>
            </section>
        </items>
    </content>
</jcr:root>
